<%- include("partials/navbar.ejs");  -%>
</section>

<section class="main_login">
  <div class="container">
      <div class="row">
          <div class="col-lg-6 col-md-6 d-none d-md-block">
              <img class="img-fluid" src="images/mobile.png" alt="....">
